URxvt.intensityStyles: false
URxvt.scrollBar: false

! Ctrl Up and Down control font size
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up

! disable ctrl-shift
URxvt.iso14755: false
URxvt.iso14755_52: false

! font
URxvt*font: xft:Source Code Pro Medium:size=8:antialias=true:hinting=true

! terminal padding
URxvt.internalBorder: 25

! clickable links, clipboard support, mouse wheel support, and on the fly font resizing
URxvt.perl-ext-common: default,vtwheel,resize-font
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg6

